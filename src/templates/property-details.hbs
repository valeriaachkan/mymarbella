<div class="property-details__container">
                    <div class="property-caption">
                        <div class="text-wrapper">
                            <h2 class="property-name">{{this.PropertyType.NameType}}</h2>
                            <p class="property-ref">{{this.Reference}}</p>
                        </div>
                        <div class="text-wrapper--spaced">
                            {{#if this.Location}}   
                                <p class="property-location--details">{{this.Location}}, {{this.Area}}, {{this.Province}}</p>
                            {{else}}
                            {{#if this.Area}} 
                                <p class="property-location--details">{{this.Area}}, {{this.Province}}</p>
                            {{else}}
                                <p class="property-location--details">{{this.Province}}</p>
                            {{/if}}
                            {{/if}}
                            {{#if this.Price}}
                                <p class="property-price--details">€{{price this.Price}}</p>
                            {{/if}}
                            {{#if this.RentalPrice2}}
                               <p class="property-price--details">{{rental-price this.RentalPrice1 this.RentalPrice2}} €{{this.RentalPrice2}} / {{this.RentalPeriod}}</p>
                            {{/if}}
                        </div>
                    </div>

                    <div class="content-wrapper property-photos slider">
                        {{#if this.Pictures}}
                        {{#each this.Pictures.Picture}}
                        <div class="property-photo__item"><img class="property-photo" src="{{this.PictureURL}}" alt="Property photo"></div>
                        {{/each}}
                        {{else}}
                        <div class="property-photo__item--placeholder"></div>
                        {{/if}}
