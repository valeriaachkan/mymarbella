                    <div class="content-wrapper property-description">
                        <div class="property-description__container">
                            <h3 class="content-wrapper__heading">Property Description</h3>
                            <p class="property-description__text">{{this.Description}}
                            </p>
                        </div>
                        <div class="property-description__container">
                            <h3 class="content-wrapper__heading">Property Details</h3>
                            <ul class="property-details__list">
                                <li class="property-details__item">
                                    Property Ref<span class="property-details__value">{{this.Reference}}</span>
                                </li>
                                {{#if this.Price}}
                                <li class="property-details__item">
                                    Price <span class="property-details__value">€{{price this.Price}}</span>
                                </li>
                               {{/if}}
                               {{#if this.RentalPrice2}}
                                <li class="property-details__item">
                                    Price <span class="property-details__value">{{rental-price this.RentalPrice1 this.RentalPrice2}} €{{this.RentalPrice2}} / {{this.RentalPeriod}}</span>
                                </li>
                               {{/if}}
                                <li class="property-details__item">
                                    Property Size <span class="property-details__value">{{this.Built}} m2</span>
                                </li>
                                <li class="property-details__item">
                                    Property Type <span class="property-details__value">{{this.PropertyType.Type}}</span>
                                </li>
                                {{#if this.YearBuilt}}
                                <li class="property-details__item">
                                    Year Built<span class="property-details__value">{{this.YearBuilt}}</span>
                                </li>
                               {{/if}}
                                <li class="property-details__item">
                                    Bedrooms <span class="property-details__value">{{this.Bedrooms}}</span>
                                </li>
                                <li class="property-details__item">
                                    Bathrooms <span class="property-details__value">{{this.Bathrooms}}</span>
                                </li>
                                {{#if this.Garage}}
                                <li class="property-details__item">
                                    Garage <span class="property-details__value">{{this.Garage}}</span>
                                </li>
                               {{/if}}
                            </ul>
                        </div>
                    </div>

                    {{#if this.PropertyFeatures.Category}}
                    <div class="content-wrapper property-amenities">
                        <h3 class="content-wrapper__heading">Features & Amenities</h3>
                        <ul class="property-amenities__list">
                        {{#each this.PropertyFeatures.Category}}
                            <li class="property-amenities__item">
                                <p class="property-amenities__type">{{this.Type}}</p>
                                <p class="property-amenities__value">{{string this.Value}}</p>
                            </li>
                        {{/each}}
