
    {{#each this}}
     <li class="property-card" data-reference="{{this.Reference}}">
        <div class="property-thumb">
            {{#if this.Pictures}}
                <img class="property-image" src="{{property-photo this.Pictures}}" alt="Property photo">
            {{/if}}
            {{#if this.Price}}
                <div class="property-price">€ {{price this.Price}}</div>
            {{else}}
                <div class="property-price">{{rental-price this.RentalPrice1 this.RentalPrice2}} €{{this.RentalPrice2}} / {{this.RentalPeriod}}</div>
            {{/if}}
        </div>

        <div class="property-content">
            <h4 class="property-title">{{this.PropertyType.NameType}}</h4>
            <p class="property-location">{{this.Location}}, {{this.Area}}, {{this.Province}}</p>
            <div class="line"></div>
